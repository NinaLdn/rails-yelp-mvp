<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.select :category, Restaurant::CATEGORY %>
  <%= f.submit "add a restaurant", class: "btn btn-primary" %>
<% end %>

<%= link_to "Go back to the list of restaurants",  restaurants_path %>
